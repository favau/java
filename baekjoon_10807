package hello;
import java.util.Scanner;

public class Variable1{
	public static void main(String[] args){
		Scanner a = new Scanner(System.in);
		int b = a.nextInt();
        int[] d = new int[b+1];
        int count=0;
        for(int i=0;i<=b;i++) {
        	d[i] = a.nextInt();
        }
        for(int j=0;j<b;j++) {
        	if(d[b]==d[j]) {
        		count++;
        	}
        }
        System.out.println(count);
	}
}
