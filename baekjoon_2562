package hello;
import java.util.Scanner;

public class Variable1{
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int[] a = new int[9];
		int MAXC=0,MAX=0;
		for(int i=0;i<9;i++) {
			a[i] = sc.nextInt();
		}
			for(int j=0;j<9;j++) {
				if(MAX<a[j]) {
					MAX = a[j];
					MAXC = j+1;
				}
			}
		System.out.println(MAX+"\n"+MAXC);
	}
}
