package hello;
import java.util.Scanner;
public class Variable1{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int k=0,sum=0;
		int X = sc.nextInt();
		int N = sc.nextInt();
		int[] a = new int[N];
		for(int j=0;j<=N-1;j++) {
			a[j] = sc.nextInt();
			k = sc.nextInt();
			sum += k*a[j];
			
		}
		if(X==sum) {
			System.out.println("Yes");
		}
		else {
			System.out.println("No");
		}
	}
}
